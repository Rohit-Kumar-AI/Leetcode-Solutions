#include <vector>
#include <algorithm>
#include <climits>
using namespace std;

class Solution {
public:
    int numberOfPairs(vector<vector<int>>& points) {
        sort(points.begin(), points.end(), [](const vector<int>& a, const vector<int>& b){
            if (a[0] != b[0]) return a[0] < b[0];
            return a[1] > b[1];
        });
        int n = points.size(), ans = 0;
        for (int i = 0; i < n; ++i) {
            int yA = points[i][1], maxY = INT_MIN;
            for (int j = i + 1; j < n; ++j) {
                int yB = points[j][1];
                if (yB <= yA && yB > maxY) {
                    ++ans;
                    maxY = yB;
                }
            }
        }
        return ans;
    }
};
