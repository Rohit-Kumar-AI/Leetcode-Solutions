class Solution {
public:
    int strToNum(string s) {
        int n = 0;
        for(char c : s) n = n * 10 + (c - 'a');
        return n;
    }

    bool isSumEqual(string firstWord, string secondWord, string targetWord) {
        return strToNum(firstWord) + strToNum(secondWord) == strToNum(targetWord);
    }
};
