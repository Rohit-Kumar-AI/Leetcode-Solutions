#include <stdbool.h>

bool carPooling(int** trips, int tripsSize, int* tripsColSize, int capacity) {
    int changes[1001] = {0};

    for (int i = 0; i < tripsSize; i++) {
        int num = trips[i][0];
        int start = trips[i][1];
        int end = trips[i][2];

        changes[start] += num;
        changes[end] -= num;
    }

    int passengers = 0;
    for (int i = 0; i <= 1000; i++) {
        passengers += changes[i];
        if (passengers > capacity) return false;
    }
    return true;
}
